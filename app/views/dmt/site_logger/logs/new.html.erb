<h1>New log</h1>

<% form_for(@log) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :controller %><br />
    <%= f.text_field :controller %>
  </p>
  <p>
    <%= f.label :action %><br />
    <%= f.text_field :action %>
  </p>
  <p>
    <%= f.label :params %><br />
    <%= f.text_field :params %>
  </p>
  <p>
    <%= f.label :browser %><br />
    <%= f.text_field :browser %>
  </p>
  <p>
    <%= f.label :ipaddr %><br />
    <%= f.text_field :ipaddr %>
  </p>
  <p>
    <%= f.label :referer %><br />
    <%= f.text_field :referer %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', dmt_site_logger_logs_path %>
